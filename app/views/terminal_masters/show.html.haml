- model_class = TerminalMaster
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:carrier_id) + ':'
  %br
  = @terminal_master.carrier_id
%p
  %strong= model_class.human_attribute_name(:maker_id) + ':'
  %br
  = @terminal_master.maker_id
%p
  %strong= model_class.human_attribute_name(:group_id) + ':'
  %br
  = @terminal_master.group_id
%p
  %strong= model_class.human_attribute_name(:model_name) + ':'
  %br
  = @terminal_master.model_name
%p
  %strong= model_class.human_attribute_name(:default_os) + ':'
  %br
  = @terminal_master.default_os
%p
  %strong= model_class.human_attribute_name(:screen_inch) + ':'
  %br
  = @terminal_master.screen_inch
%p
  %strong= model_class.human_attribute_name(:screen_width) + ':'
  %br
  = @terminal_master.screen_width
%p
  %strong= model_class.human_attribute_name(:screen_height) + ':'
  %br
  = @terminal_master.screen_height
%p
  %strong= model_class.human_attribute_name(:screen_density_id) + ':'
  %br
  = @terminal_master.screen_density_id
%p
  %strong= model_class.human_attribute_name(:release_date) + ':'
  %br
  = @terminal_master.release_date
%p
  %strong= model_class.human_attribute_name(:tablet) + ':'
  %br
  = @terminal_master.tablet
%p
  %strong= model_class.human_attribute_name(:nfc) + ':'
  %br
  = @terminal_master.nfc


= link_to t('.back', :default => t("helpers.links.back")), terminal_masters_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_terminal_master_path(@terminal_master), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), terminal_master_path(@terminal_master), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
