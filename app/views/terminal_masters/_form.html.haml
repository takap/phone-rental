= simple_form_for @terminal_master, :html => { :class => 'form-horizontal' } do |f|
  = f.input :carrier_id
  = f.input :maker_id
  = f.input :series_id
  = f.input :model_name
  = f.input :default_os_id
  = f.input :screen_inch
  = f.input :screen_width
  = f.input :screen_height
  = f.input :screen_density_id
  = f.input :release_date
  = f.input :tablet
  = f.input :nfc
  = f.button :submit, :class => 'btn-primary'
  = link_to t('.cancel', :default => t("helpers.links.cancel")), terminal_masters_path, :class => 'btn btn-default'
